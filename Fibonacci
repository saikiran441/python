x=0
y=1
count=0
n=int(input("Enter no of terms in sequence: "))
if(n<=0):
    print("Please enter a positive integer: ") 
elif(n==1):
    print("Fibonacci sequence is: ",x)
else:
    print("Fibonacci sequence is: ")
    while count<n:
        print(x)
        temp=x+y
        x=y
        y=temp
        count+=1
        

    
